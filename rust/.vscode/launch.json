    "configurations": [
        {
            "name": "Debug ja4",
            "type": "lldb-dap",
            "request": "launch",
            "preLaunchTask": "cargo build (debug)",
            "program": "${workspaceFolder}/target/debug/ja4",
            "args": [
                "${workspaceFolder}/../pcap/tcp.pcap",
                "-j"
            ],
            "cwd": "${workspaceFolder}",
            "env": {
                "HOME": "${env:HOME}",
                "RUST_LOG": "debug",
            },
        }
    ]
}